{
  "version": 2,
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.js"
    },
    {
      "src": "/v1/transaction/",
      "dest": "src/routes/transaction.route.js",
      "methods": ["POST"]
    }
  ]
}
