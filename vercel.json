{
  "version": 2,
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.js"
    },
    {
      "src": "/v1/transaction/",
      "dest": "src/routes/transaction.route.js",
      "methods": ["POST", "GET"]
    },
    {
      "src": "/v1/user/",
      "dest": "src/routes/user.route.js",
      "methods": ["POST", "GET"]
    },
    {
      "src": "/v1/wallet/",
      "dest": "src/routes/wallet.route.js",
      "methods": ["POST", "GET"]
    },
    {
      "src": "/v1/goal/",
      "dest": "src/routes/goal.route.js",
      "methods": ["POST", "GET"]
    }
  ]
}
